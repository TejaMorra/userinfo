  "main": "dist/server.js",
  "scripts": {
    "build-ts": "tsc",
    "start": "npm run copySwaggerDoc && npm run build-ts && npm run serve",
    "start:prod": "npm run build-ts && npm run copySwaggerDoc && npm run prod-serve",
    "prod-serve": "NODE_ENV=Prod  node dist/server.js",
    "serve": "NODE_ENV=Dev node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "swagger:edit": "swagger project edit",
    "copySwaggerDoc": "cpx \"src/swagger-docs/*.*\" dist/swagger-docs",
